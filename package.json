{
  "name": "baileys",
  "version": "6.7.18",
  "type": "module",
  "description": "A WebSockets library for interacting with WhatsApp Web",
  "keywords": [
    "whatsapp",
    "automation"
  ],
  "homepage": "https://github.com/WhiskeySockets/Baileys/",
  "repository": {
    "url": "git@github.com:WhiskeySockets/Baileys.git"
  },
  "license": "MIT",
  "author": "Rajeh Taher",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib/*",
    "WAProto/*",
    "engine-requirements.js"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-esm-fix --target=lib",
    "build:all": "npm run build && typedoc",
    "build:docs": "typedoc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "example": "node --import tsx Example/example.ts",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "npm run format && npm run lint -- --fix",
    "postinstall": "node -e \"console.log('Using pre-built lib/ folder from repository')\"",
    "prepare": "node -e \"console.log('Skipping TypeScript compilation - using committed lib/ folder')\"",
    "release": "release-it",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.e2e.config.js",
    "validate:lib-sync": "node scripts/validate-lib-sync.js",
    "prerelease": "npm run validate:lib-sync",
    "config:check": "node scripts/check-baileys-config.js"
  },
  "dependencies": {
    "@cacheable/node-cache": "^1.4.0",
    "@hapi/boom": "^9.1.3",
    "async-mutex": "^0.5.0",
    "audio-decode": "^2.1.3",
    "libsignal": "github:whiskeysockets/libsignal-node",
    "lru-cache": "^10.4.3",
    "music-metadata": "^10.0.0",
    "p-queue": "^9.1.0",
    "pino": "^9.6",
    "prom-client": "^15.1.3",
    "protobufjs": "^7.2.4",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@whiskeysockets/eslint-config": "github:whiskeysockets/eslint-config",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "jimp": "^1.6.0",
    "json": "^11.0.0",
    "link-preview-js": "^3.0.0",
    "open": "^8.4.2",
    "prettier": "^3.5.3",
    "protobufjs-cli": "^1.1.3",
    "release-it": "^15.10.3",
    "sharp": "^0.34.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.8.1",
    "tsc-esm-fix": "^3.0.2",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "4.4.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "audio-decode": "^2.1.3",
    "jimp": "^1.6.0",
    "link-preview-js": "^3.0.0",
    "sharp": "^0.34.2"
  },
  "peerDependenciesMeta": {
    "audio-decode": {
      "optional": true
    },
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20.0.0"
  }
}
